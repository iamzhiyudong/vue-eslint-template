{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:vue/recommended", "prettier"],
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "plugins": ["vue", "prettier"],
  "rules": { "prettier/prettier": ["error"], "import/no-unresolved": "off" }
}
